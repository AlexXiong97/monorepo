{
  "name": "counterfactual-monorepo",
  "private": true,
  "engines": {
    "node": ">=8.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "ganache": "ganache-cli --networkId 7777777 --gasLimit 0xfffffffffff --port ${npm_package_config_ganachePort} --deterministic --account=\"${npm_package_config_unlockedAccount0},${npm_package_config_etherBalance}\" --account=\"${npm_package_config_unlockedAccount1},${npm_package_config_etherBalance}\" --account=\"${npm_package_config_unlockedAccount2},${npm_package_config_etherBalance}\"",
    "wsrun": "wsrun",
    "build": "wsrun build $PKG --fast-exit -r --stages --exclude-missing",
    "test": "wsrun test $PKG --fast-exit -r --stages --exclude-missing"
  },
  "config": {
    "ganachePort": 9545,
    "unlockedAccount0": "0xf2f48ee19680706196e2e339e5da3491186e0c4c5030670656b0e0164837257d",
    "unlockedAccount1": "0xf2f48ee19680706196e2e339e5da3491186e0c4c5030670656b0e0164837257e",
    "unlockedAccount2": "0xf2f48ee19680706196e2e339e5da3491186e0c4c5030670656b0e0164837257f",
    "etherBalance": "1000000000000000000000000"
  },
  "devDependencies": {
    "ganache-cli": "^6.1.8",
    "jest": "^23.6.0",
    "lerna": "^2.11.0",
    "typescript": "^3.1.2",
    "wsrun": "^2.2.1"
  }
}
